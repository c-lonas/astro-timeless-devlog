---

const { url } = Astro.props;
const { title } = Astro.props;

---

<a href={url}>
    <li class="group hover:bg-midnight-800 rounded-lg pl-4 py-2">
        <p class="transition-transform ease-in-out duration-300 group-hover:translate-x-[-5px] group-hover:text-midnight-200"> {title} </p>
    </li>
</a>
